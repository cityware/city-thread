{
    "name": "cityware/city-thread",
    "description": "Cityware Thread",
    "license": "GPL-3.0",
    "keywords": [
        "cityware",
        "thread"
    ],
    "authors": [
        {
            "name": "Fabricio S V Xavier",
            "email": "fabricio@cityware.com.br",
            "homepage": "http://www.cityware.com.br",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cityware/city-thread.git"
        }
    ],
    "homepage": "https://github.com/cityware/city-thread",
    "autoload": {
        "psr-4": {
            "Cityware\\Thread\\": "src/"
        },
        "psr-0": {
            "Cityware\\Thread\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
    },
    "suggest": {
        "ext-intl": "Required for most features of Zend\\I18n; included in default builds of PHP"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CitywareTest\\Thread\\": "test/"
        }
    }
}